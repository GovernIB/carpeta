            <%-- FILTRE NUMERO DESDE-FINS --%>
              <span class="add-on"><fmt:message key="enllaz.seccioID" />:</span>

              <span class="add-on">&nbsp;<fmt:message key="genapp.from" /></span>
              
              <form:input cssClass="input-append input-small" path="seccioIDDesde" />


