{
  "expo": {
    "name": "CarpetaApp GOIB",
    "slug": "CarpetaApp",
    "version": "1.0.7",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "carpetaapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "packagerOpts": {
      "sourceExts": [
        "web.js",
        ".js"
      ]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "es.caib.carpeta.carpetaapp",
      "buildNumber": "1.0.7",
      "infoPlist": {
        "CFBundleLocalizations": [
          "ca",
          "es"
        ],
        "CFBundleDevelopmentRegion": "es"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "es.caib.carpeta.carpetaapp",
	    "blockedPermissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "ACCESS_BACKGROUND_LOCATION"],
      "googleServicesFile": "./google-services.json",	  
      "versionCode": 7
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e1d384d1-b819-4017-a131-52c6855214a5"
      }
    }
  }
}
